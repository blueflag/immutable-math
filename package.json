{
  "name": "immutable-math",
  "version": "0.1.0",
  "description": "Some maths for immutable collections.",
  "main": "lib/index.js",
  "repository": "github:blueflag/immutable-math",
  "scripts": {
    "dev": "./node_modules/.bin/babel src -d lib --watch",
    "build": "./node_modules/.bin/babel src --out-dir lib && npm run docs",
    "tape": "tape -r ./tests/tests.js src/**/*-test.js",
    "test": "nyc --report-dir=./coverage npm run tape && npm run check-coverage",
    "check-coverage": "nyc check-coverage --branches 0.0  --functions 0.0 --lines 0",
    "docs": "jsdoc -c jsdoc/conf.json",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "keywords": [
    "immutable",
    "immutable.js",
    "maths"
  ],
  "author": "Damien Clarke",
  "license": "MIT",
  "devDependencies": {
    "app-module-path": "^1.1.0",
    "babel-cli": "^6.5.0",
    "babel-core": "^6.5.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-config-blueflag": "^0.1.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "flow-bin": "^0.33.0",
    "glob": "^7.0.6",
    "immutable": "^3.8.1",
    "jsdoc": "^3.4.1",
    "jsdoc-babel": "^0.2.1",
    "jsdonk": "^0.1.2",
    "nyc": "^8.1.0",
    "repeating": "^2.0.0",
    "tape": "^4.6.0"
  },
  "peerDependencies": {
    "immutable": "^3.8.1"
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**"
    ],
    "exclude": [
      "src/**/*-test.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "require": [
      "./tests/tests.js"
    ]
  }
}
